{
  "name": "armature-core",
  "version": "0.1.4",
  "description": "A fullstack web framework using Bun",
  "main": "dist/src/index.js",
  "module": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "type": "module",
  "bin": { "armature": "./dist/src/cli/index.js" },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "exports": {
    ".": "./dist/index.js",
    "./core": "./dist/src/index.js",
    "./api": "./dist/src/api/index.js",
    "./jsx-runtime": "./dist/src/jsx/jsx-runtime",
    "./jsx-dev-runtime": "./dist/src/jsx/jsx-dev-runtime"
  },
  "scripts": {
    "dev": "bun run --watch ./src/server/index.ts",
    "start": "bun run src/cli/index.ts dev",
    "build": "swc . -d dist --ignore dist,node_modules,plugins && tsc --emitDeclarationOnly --outDir dist && chmod +x dist/src/cli/index.js",
    "prepublishOnly": "bun run build"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/chokidar": "^2.1.3",
    "@types/node": "^22.2.0"
  },
  "peerDependencies": {
    "typescript": "^5.5.4"
  },
  "dependencies": {
    "@elysiajs/eden": "^1.1.2",
    "@elysiajs/html": "1.0.0",
    "@elysiajs/jwt": "^1.1.0",
    "@elysiajs/static": "^1.1.0",
    "@elysiajs/swagger": "^1.1.1",
    "@swc/cli": "^0.4.0",
    "@swc/core": "^1.7.10",
    "chalk": "^5.3.0",
    "chokidar": "^3.6.0",
    "elysia": "^1.1.5",
    "jsx-to-html-runtime": "1.0.7",
    "svelte": "^4.2.18",
    "terser": "^5.31.5",
    "vite": "^5.4.0"
  }
}
